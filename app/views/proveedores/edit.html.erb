<h1 class="titulo">editar proveedor</h1>

<div class="row">
    <div class="col-md-4">
        <%= simple_form_for @proveedor do |f| %>
            <%= f.input :codigo %>
            <%= f.input :nombre %>
            <%= f.input :correoelectronico %>
            <%= f.input :telefono %>
            <%= f.input :direccion %>
            <%= f.input :ciudad %>
            <%= f.input :destinario %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div>