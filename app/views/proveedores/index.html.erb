<h1>Listing proveedor</h1>
<%= link_to 'New proveedor', new_article_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="3"></th>
  </tr>
 
<% @proveedor.each do |proveedor| %>
  <tr>
    <td><%= article.title %></td>
    <td><%= article.text %></td>
    <td><%= link_to 'Show', proveedor_path(proveedor) %></td>
    <td><%= link_to 'Edit', edit_proveedor_path(proveedor) %></td>
    <td><%= link_to 'Destroy', proveedor_path(proveedor),
       method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>