<h1 class="titulo">Registro de proveedor</h1>
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= simple_form_for @proveedor do |f| %>
            <%= f.input :codigo %>
            <%= f.input :nombre %>
            <%= f.input :correoelectronico %>
            <%= f.input :telefono %>
            <%= f.input :direccion %>
            <%= f.input :ciudad %>
            <%= f.input :destinario %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div>




