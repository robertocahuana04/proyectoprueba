<h1 class="page-header">Registro de proveedor</h1>
<div class="center jumbotron">
    <div class="container">
        <%= simple_form_for @proveedor do |f| %>
            <%= f.input :codigo %>
            <%= f.input :nombre %>
            <%= f.input :correoelectronico %>
            <%= f.input :telefono %>
            <%= f.input :direccion %>
            <%= f.input :ciudad %>
            <%= f.input :destinario %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div>



