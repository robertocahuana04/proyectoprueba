<h1 class="titulo">nuevo productos</h1>
<div class="row">
    <div class="col-md-4">
        <%= simple_form_for @producto do |f| %>
            <%= f.input :codigo %>
            <%= f.input :nombre %>
            <%= f.input :referencia %>
            <%= f.input :stock %>
            <%= f.input :remitente %>
            <%= f.input :tipo %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div>