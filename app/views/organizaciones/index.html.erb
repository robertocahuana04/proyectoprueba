<h1 class="text-center , fs-1">Listing reclamos</h1>

<div>
<%= link_to 'crear organizacione', new_organizacione_path, class: 'btn btn-primary'  %>
</div>


<table class = "table table-bordered table-hover">
  <tr>
    <th>tipo_de_organizacion</th>
    <th>numero_de_empleado</th>
    <th>representantes</th>
    <th colspan="3">Accion</th>
  </tr>

  <% @organizacione.each do |organizacione| %>
    <tr>
      <td><%= organizacione.tipo_de_organizacion %></td>
      <td><%= organizacione.numero_de_empleado %></td>
      <td><%= organizacione.representantes %></td>
      <td><%= link_to 'Ver', organizacione_path(organizacione), class: 'btn btn-success' %></td>
      <td><%= link_to 'Editar', edit_organizacione_path(organizacione), class: 'btn btn-primary' %></td>
      <td><%= link_to 'Eliminar', organizacione_path(organizacione),
      method: :delete, data: { confirm: 'Estas seguro?' }, :class => 'btn btn-danger' %></td>
    </tr>
  <% end %>
</table>

