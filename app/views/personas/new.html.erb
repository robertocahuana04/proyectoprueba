<h1 class="titulo">nuevo personas</h1>
<div class="center jumbotron">
    <div class="container">
        <%= simple_form_for @persona do |f| %>
            <%= f.input :cedula %>
            <%= f.input :genero %>
            <%= f.input :fecha_nacimiento %>
            <%= f.input :telefono %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div> 