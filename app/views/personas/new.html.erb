<h1 class="text-center , fs-1">nuevo personas</h1>

<div class="row">
    <div class="col-md-6">
        <%= simple_form_for @persona do |f| %>
            <%= f.input :cedula %>
            <%= f.input :genero %>
            <%= f.input :fecha_nacimiento %>
            <%= f.input :telefono %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div> 

