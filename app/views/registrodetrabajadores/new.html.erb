<h1 class="titulo">Registro de trabajadores</h1>
<div class="container">
    <div class="navbar-toggler">
        <%= simple_form_for @registrodetrabajadore do |f| %>
            <%= f.input :nombre %>
            <%= f.input :edad %>
            <%= f.input :cargo %>
            <%= f.input :telefono %>
            <%= f.button :submit %>
        <% end %>
    </div>
</div>
