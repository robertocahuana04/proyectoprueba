<h1>Listing Registrodetrabajadores</h1>
<%= link_to 'New trabajador', new_registrodetrabajadore_path %>
<table>
  <tr>
    <th>nombre</th>
    <th>edad</th>
    <th>cargo</th>
    <th>telefono</th>
    <th colspan="4"></th>
  </tr>
 
  <% @registrodetrabajadore.each do |registrodetrabajadore| %>
    <tr>
      <td><%= registrodetrabajadore.nombre %></td>
      <td><%= registrodetrabajadore.edad %></td>
      <td><%= registrodetrabajadore.cargo %></td>
      <td><%= registrodetrabajadore.telefono %></td>
      <td><%= link_to 'Show', registrodetrabajadore_path(registrodetrabajadore) %></td>
      <td><%= link_to 'Editar', edit_registrodetrabajadore_path(registrodetrabajadore) %></td>
      <td><%= link_to 'Destroy', registrodetrabajadore_path(registrodetrabajadore),
      method: :delete, data: { confirm: 'Estas seguro?' } %></td>
    </tr>
  <% end %>
</table>