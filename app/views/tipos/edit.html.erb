<p class="text-center , fs-1">editar tipos</p>

<%= link_to 'listado tipo', tipo_path, class: 'btn btn-primary' %>
<%= link_to 'Back', tipos_path, class: 'btn btn-primary' %>

<%= form_for :post do |f| %>
    <p>
      <%= f.label :tipo %><br>
      <%= f.text_field :tipo %>
    </p>

    <p>
      <%= f.label :estado %><br>
      <%= f.text_area :estado %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>