<p id="notice"> <%= notice%> </p>
<p class="text-center , fs-1">listado de stoockdeproducto</p>

<div class="container">
<%= link_to 'Editar', edit_stoockdeproducto_path(@stoockdeproducto), class: 'btn btn-primary' %>
