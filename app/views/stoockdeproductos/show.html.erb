            <th>codigo</th>
            <th>nombre</th>
            <th>cantidad_existente</th>
            <th>referencia</th>
            <th>tipo</th>
            <th>proveedor</th>
        </tr>
        <tr>
            <td><%= @stoockdeproducto.codigo%></td>
            <td><%= @stoockdeproducto.nombre%></td>
            <td><%= @stoockdeproducto.cantidad_existente%></td>
            <td><%= @stoockdeproducto.referencia%></td>
            <td><%= @stoockdeproducto.tipo %></td>
            <td><%= @stoockdeproducto.proveedor%></td>
        </tr>
    </table> 
